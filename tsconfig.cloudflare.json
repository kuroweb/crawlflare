{
  "extends": "./tsconfig.json",
  "include": [
    ".react-router/types/**/*",
    "frontend/**/*",
    "backend/**/*",
    "worker-configuration.d.ts"
  ],
  "compilerOptions": {
    "checkJs": false,
    "composite": true,
    "strict": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2022"
    ],
    "types": [
      "vite/client"
    ],
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "baseUrl": ".",
    "rootDirs": [
      ".",
      "./.react-router/types"
    ],
    "paths": {
      "~/*": [
        "./frontend/*"
      ]
    },
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
